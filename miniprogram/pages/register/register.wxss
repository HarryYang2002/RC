/* pages/register/register.wxss */
.lic-tmpl {
    width: 600rpx;
    height: 450rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}

.stretch {
    width: 100%;
    height: 100%;
}

.lic-tmpl-img {
    position: absolute;
    opacity: .2;
}

.instruction {
    position: absolute;
    bottom: -74rpx;
}

.cuIcon-cameraadd {
    font-size: 173rpx;
}

.container {
    padding-top: 65rpx;
}

.lic-form {
    width: 600rpx;
}

.cu-form-group .title {
    min-width: calc(4em + 60rpx);
}

.check {
    position: absolute;
    right: 0;
    top: 0;
    width: 100rpx;
    height: 100rpx;
}